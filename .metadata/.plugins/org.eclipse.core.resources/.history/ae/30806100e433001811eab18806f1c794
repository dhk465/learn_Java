package ex2_map;

import java.util.HashMap;
import java.util.Scanner;

public class Ex2_Map {
	public static void main(String[] args) {
		
		HashMap<String, Integer> user = new HashMap<>();
		user.put("kim", 1111);
		user.put("lee", 2222);
		user.put("park", 3333);
		
		String id;
		int pwd;
		
		Scanner sc = new Scanner(System.in);
		
		System.out.print("ID: ");
		id = sc.next();
		System.out.print("Password: ");
		pwd = sc.nextInt();
		
	}
}
