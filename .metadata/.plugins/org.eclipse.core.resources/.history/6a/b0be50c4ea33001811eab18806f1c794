package ex3_arraylist;

import java.util.ArrayList;

public class Ex1_ArrayList {
	public static void main(String[] args) {
		
		ArrayList<Integer> list = new ArrayList<>();
		list.add(150);
		list.add(110);
		list.add(100);
		list.add(110); // 중복된 값 허용
		System.out.println(list.size());
		
		
		
	}
}
