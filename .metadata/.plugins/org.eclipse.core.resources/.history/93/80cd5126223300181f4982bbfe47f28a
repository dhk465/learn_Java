package ex5_daemon;

public class DaemonTest implements Runnable {

	@Override
	public void run() {
		
		while (true) {
			
			try {
				Thread.sleep(5000);
				System.out.println("저장완료");
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			
		}
		
	}
	
}
