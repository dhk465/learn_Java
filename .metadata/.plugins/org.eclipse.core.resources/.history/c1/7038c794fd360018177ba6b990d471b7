package ex_work;

public class TypeGame extends new Runnable() {

	public void run() {
		
	}
	
	/*
	준비해놓은 문자열 배열의 값이 3초 간격으로 arrayList에 추가
	
	String[] data = { "apple", "orange", "grape" };
	
	------------------------
	[orange]
	>> orange
	[apple]
	>> apple
	[grape, apple]
	>> apple
	[grape]
	>> grape
	[orange]
	>> orange
	클리어!!!
	
	 */
	
	
	
}
