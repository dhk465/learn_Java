package ex1_set;

import java.util.HashSet;
import java.util.Random;

public class Ex2_iterator {
	public static void main(String[] args) {
		
		HashSet<Integer> hs = new HashSet<>();
		
		int[] arr = new int[6];
		
		while (true) {
			
			hs.add(new Random().nextInt(45) + 1);
			
			if (hs.size() >= 6) {
				break;
			}
			
		} // while
		
		System.out.println(hs);
		
	}
}
