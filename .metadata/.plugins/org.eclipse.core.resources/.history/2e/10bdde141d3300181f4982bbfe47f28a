package ex4_thread;

public class ThreadCount implements Runnable {
	
	private int num;
	
	public void sendParameter(int num) {
		this.num = num;
	}
	
	@Override
	public void run() {
		
		// 정수를 입력받고
		// 입력받은 숫자를 1씩 감소
		// 0이 되면 종료메시지 출력
		// 단, 값을 받는 코드는 main에서
		// 1씩 감소시키는 코드는 ThreadCount에서
		
		for (int i = 0; i < ; i++) {
			
		}
		
	}
	
}
