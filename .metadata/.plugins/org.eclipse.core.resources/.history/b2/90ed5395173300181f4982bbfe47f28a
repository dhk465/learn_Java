package ex2_throws;

import java.util.Scanner;

public class Test {
	public void getRes() {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.print("정수: ");
		int num;
		try {
			num = sc.nextInt();
			System.out.println("결과: " + num);
		} catch (Exception e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		
		
	}
}
